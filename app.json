{
  "expo": {
    "name": "hospitalClinico",
    "slug": "hospitalClinico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hospital.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/hospital.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hospital.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.danywolf.hospitalClinico"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f1bcb26c-51c9-4529-ba1c-7ec1bffe86b7"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ]
  }
}
